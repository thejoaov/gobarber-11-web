{
  "name": "6-iniciando-frontend-web",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "dependencies": {
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@unform/core": "^2.1.3",
    "@unform/web": "^2.1.3",
    "axios": "^0.19.2",
    "polished": "^3.6.4",
    "react": "^16.13.1",
    "react-app-rewired": "^2.1.8",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "react-spring": "^8.0.27",
    "styled-components": "^5.1.1",
    "typescript": "^4.1.3",
    "uuid": "^8.3.2",
    "yup": "^0.29.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/react-hooks": "^5.0.3",
    "@testing-library/user-event": "^7.1.2",
    "@types/axios": "^0.14.0",
    "@types/jest": "^24.0.0",
    "@types/react-router-dom": "^5.1.6",
    "@types/styled-components": "^5.1.0",
    "@types/uuid": "^8.3.0",
    "@types/yup": "^0.29.3",
    "@typescript-eslint/eslint-plugin": "^2.32.0",
    "@typescript-eslint/parser": "^2.32.0",
    "eslint": "6.6.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "git-commit-msg-linter": "^3.0.0",
    "husky": "^4.3.6",
    "jest-localstorage-mock": "^2.4.6",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "react-hooks-testing-library": "^0.6.0",
    "react-test-renderer": "^17.0.1"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "eject": "react-app-rewired eject",
    "lint": "eslint . --ext \"js,ts,tsx\" --cache",
    "prettify": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json}\" --write",
    "test": "react-app-rewired test --watchAll=false --runInBand",
    "test:watch": "yarn test --watch",
    "test:changed": "yarn test -o --passWithNoTests",
    "test:coverage": "yarn test --coverage",
    "test:ci": "TZ=America/Fortaleza yarn test --ci --runInBand --collectCoverage --passWithNoTests"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^react-spring(.*)$": "<rootDir>/__mocks__/react-spring-mock.js",
      "^@src/(.*)$": "<rootDir>/src/$1",
      "^@assets/(.*)$": "<rootDir>/src/assets/$1",
      "^@components": "<rootDir>/src/components",
      "^@config/(.*)$": "<rootDir>/src/config/$1",
      "^@hooks/(.*)$": "<rootDir>/src/hooks/$1",
      "^@pages/(.*)$": "<rootDir>/src/pages/$1",
      "^@routes/(.*)$": "<rootDir>/src/routes/$1",
      "^@services/(.*)$": "<rootDir>/src/services/$1",
      "^@styles/(.*)$": "<rootDir>/src/styles/$1",
      "^@utils/(.*)$": "<rootDir>/src/utils/$1",
      "^@hooks": "<rootDir>/src/hooks"
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "(styles|types).ts",
      "src/index.tsx",
      "src/components/index.ts"
    ]
  }
}
